<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="taskcluster"
    fetch="https://github.com/taskcluster/" />
  <default revision="master"
           remote="taskcluster"
           sync-j="4" />

  <project path="queue" name="taskcluster-queue" />
  <project path="index" name="taskcluster-index" />
  <project path="base" name="taskcluster-base" />
  <project path="client" name="taskcluster-client" />
  <project path="client.py" name="taskcluster-client.py" />
  <project path="docker-worker" name="docker-worker" />
  <project path="cli" name="taskcluster-cli" />
  <project path="aws-provisioner" name="aws-provisioner" />
  <project path="docs" name="taskcluster-docs" />
  <project path="scheduler" name="task-graph-scheduler" />
  <project path="events" name="taskcluster-events" />
  <project path="try" name="taskcluster-try" />
  <project path="auth" name="taskcluster-auth" />
  <project path="treeherder" name="taskcluster-treeherder" />

</manifest>
